{
  "expo": {
    "name": "Efuafinex Hair App",
    "slug": "efuafinex_hair_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#FAFAFA",
    "ios": {
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app would like to get access to your camera to take photos for your profile or posts",
        "NSPhotoLibraryAddUsageDescription": "This app would like to save images to your device",
        "NSPhotoLibraryUsageDescription": "This app would like to save images to your device",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1051061091644-bjcue7e6li5vc56fg09ioodp1dgiqcgt"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1051061091644-bjcue7e6li5vc56fg09ioodp1dgiqcgt"
            ]
          }
        ]
      },
      "bundleIdentifier": "com.polyboticstech.efuafinexhair"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "navigationBarColor": "#FAFAFA",
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.polyboticstech.efuafinexhair"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAFAFA"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to photos to upload photo(s) from your library"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f737132-24e3-44b3-9d11-b686ea592a03"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7f737132-24e3-44b3-9d11-b686ea592a03"
    }
  }
}
