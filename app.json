{
  "expo": {
    "name": "Efuafinex Hair App",
    "slug": "efuafinex_hair_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#FAFAFA",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app would like to get access to your camera to take photos for your profile or posts",
        "NSPhotoLibraryAddUsageDescription": "This app would like to save images to your device",
        "NSPhotoLibraryUsageDescription": "This app would like to save images to your device"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#008080"
      },
      "navigationBarColor": "#FAFAFA",
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.polyboticstech.efuafinex_hair_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#008080"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to photos to upload photo(s) from your library"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f737132-24e3-44b3-9d11-b686ea592a03"
      }
    }
  }
}
